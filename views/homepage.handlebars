{{#each posts as |post|}}
  <div class="row mb-4 project">
    <div class="col-md-5">
      <h2>
        <a href="/project/{{project.id}}">{{title}}</a>
      </h2>
      <div>
        <p>{{post_content}}</p>
        <p>By: <span>{{user.username}}</span> on {{date_created}}</p>
      </div>
    </div>
  </div>
{{/each}}